//Scripted pipeline
//Declarative pipeline

pipeline {
  agent any

  stages {
    stage("build") {
      steps {
        echo 'This is build stage - Executing'
      }
    }

    stage("test") {
      steps {
        echo 'This is test stage - Executing'
      }
    }

    stage("deploy") {
      steps {
        echo 'This is deploy stage - Executing'
      }
    }

  }
}